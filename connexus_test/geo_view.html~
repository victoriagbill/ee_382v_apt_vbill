		<script type="text/javascript">
       $(function() { 
					demo.add(function() {
					$('#map_canvas').gmap({'center': '30.282788,-97.553101', 'zoom': 1, 'disableDefaultUI':true, 'callback': function() {
						var self = this;
						self.addMarker({'position': this.get('map').getCenter() }).mouseover(function() {
							self.openInfoWindow({ 'content': '<img src="img/Cover.jpg" height="50px">' }, this);
						});
						self.addMarker({'position': '53.330873,-96.734619', 'bounds': false }).click(function() {
							self.openInfoWindow({ 'content': '<img src="img/Cover.jpg">' }, this);
						});
						self.addMarker({'position': '53.540307,-7.437744', 'bounds': false }).click(function() {
							self.openInfoWindow({ 'content': '<img src="img/Cover.jpg">' }, this);
						});
						self.addMarker({'position': '32.694866,-117.171936', 'bounds': false }).mouseover(function() {
							self.openInfoWindow({ 'content': '<img src="img/Cover.jpg">' }, this);
						});
						

					}});
				}).load();			
			});


   	</script> 





